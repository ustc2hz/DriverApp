<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ustc.sse.water.activity"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <!-- 使用AMap所需的相关权限 -->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <!-- 允许程序改变网络连接状态 -->
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <!-- 读取联系人权限，上传联系人需要用到此权限 -->
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <!-- 获取手机录音机使用权限，听写、识别、语义理解需要用到此权限 -->
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >

        <!-- AMap的key -->
        <meta-data
            android:name="com.amap.api.v2.apikey"
            android:value="ee23a53ae87ff81d86c30bb57a2aae54" />

        <!-- 地图界面 -->

        <activity
            android:name=".DriverMainScreen"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Light.NoTitleBar" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 车生活界面 -->

        <activity
            android:name=".DriverLife"
            android:label="@string/happy_driver_life"
            android:theme="@android:style/Theme.Dialog" >
        </activity>
        <!-- 处理导航功能的中间层，空界面 -->
        <activity
            android:name=".NaviStartActivity"
            android:label="@string/title_activity_navi_start" >
        </activity>
        <!-- 导航界面 -->
        <activity android:name=".DriverNaviActivity" >
        </activity>
        <!-- 管理员主界面 -->
        <activity
            android:name="ustc.sse.water.zf.Manager_main"
            android:label="@string/title_activity_manager_main" >
        </activity>
        <!-- 驾驶员管理员登陆界面 -->
        <activity
            android:name="ustc.sse.water.zf.Manager_Driver_model"
            android:label="@string/app_name" >
        </activity>
        <!-- 管理员主界面 -->
        <activity
            android:name="ustc.sse.water.zf.ManagerMain"
            android:label="@string/app_name" >
        </activity>

        <!-- 驾驶员管理员登陆界面 -->
        <activity
            android:name="ustc.sse.water.zf.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="ustc.sse.water.zf.ManagerMainScreen"
            android:label="@string/app_name" >
        </activity>
        <!-- 登录界面 -->
        <activity
            android:name="ustc.sse.water.zf.LoginActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="ustc.sse.water.zf.RegisterActivity"
            android:label="@string/app_name" >
        </activity>
        <!-- 管理员主界面 -->
        <activity
            android:name="ustc.sse.water.manager.zf.ManagerMainTabActivity"
            android:label="@string/app_name" >
        </activity>
        <!-- 默认订单界面 -->
        <activity
            android:name="ustc.sse.water.manager.zf.AActivity"
            android:label="@string/app_name" >
        </activity>
        <!-- 发布消息界面 -->
        <activity
            android:name="ustc.sse.water.manager.zf.BActivity"
            android:label="@string/app_name" >
        </activity>
        <!-- 个人设置界面 -->
        <activity
            android:name="ustc.sse.water.manager.zf.CActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="ustc.sse.water.manager.zf.DActivity"
            android:label="@string/app_name" >
        </activity>
        <!-- 发布消息界面 -->
        <activity
            android:name="ustc.sse.water.zf.ManagerDistributeMessage"
            android:label="@string/welcome" >
        </activity>
        <!-- 管理员个人设置界面 -->
        <activity
            android:name="ustc.sse.water.zf.ManagerSettings"
            android:label="@string/welcome">
          
       </activity>
       
        <!-- 修改密码 -->
        <activity
            android:name="ustc.sse.water.zf.ManagerChangePass"
            android:label="@string/welcome" >
        </activity>
        <!-- 接收订单 -->
        <activity
            android:name="ustc.sse.water.zf.Ordering"
            android:label="@string/welcome" >
        </activity>

        <!-- 添加车牌号 -->
        <activity android:name=".zjx.AddDriverLicence" >
        </activity>
        <!-- 驾驶员信息界面 -->
        <activity android:name=".zjx.DriverInfo" >
        </activity>
        <!-- 驾驶员订单界面 -->
        <activity android:name=".zjx.DriverOrderInfo" >
        </activity>
        <!-- 停车场详情和预定 -->
        <activity android:name=".zjx.ParkingDetail" >
        </activity>
        <!-- 停车场列表 -->
        <activity android:name="ustc.sse.water.activity.zjx.ParkingList"></activity>
        
        <!-- 更新订单的Service -->
        <service android:name="ustc.sse.water.service.UpdateOrderService"></service>
        <activity
            android:name=".MySpinner"
            android:label="@string/title_activity_my_spinner" >
        </activity>
        <activity
            android:name=".MapForAddress"
            android:label="@string/title_activity_map_for_address" >
        </activity>
    </application>

</manifest>