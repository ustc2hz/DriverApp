<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">  
  
    <style name="DriverLifeTheme" parent="@android:style/Theme.Holo.Dialog">  
        <item name="android:actionBarStyle">@style/MyActionBar</item>  
        <item name="android:actionBarDivider">@null</item>  
  
    </style>  
  
    <!-- ActionBar styles -->  
    <style name="MyActionBar" parent="@android:style/Widget.ActionBar">  
        <item name="android:background">@color/anotherBlue</item>  
        <item name="android:titleTextStyle">@style/MyActionBarTitleText</item>  
  
    </style>  
  
    <!-- ActionBar title text -->  
    <style name="MyActionBarTitleText" parent="@android:style/Widget.ActionBar">  
        <item name="android:textColor">@android:color/white</item>  
    </style>  
  
  
</resources> 
